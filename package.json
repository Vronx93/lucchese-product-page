{
  "name": "lucchese-product-page",
  "packageManager": "yarn@4.6.0",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@shopify/prettier-plugin-liquid": "^1.9.3",
    "@types/alpinejs__intersect": "^3",
    "@types/node": "^24.0.13",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "3",
    "typescript": "^5.8.3",
    "vite": "^4.3.9",
    "vite-plugin-static-copy": "^3.1.1"
  },
"scripts": {
  "tailwind:build": "tailwindcss -i ./src/styles/tailwind.css -o ./public/dist/output.css --minify",
  "tailwind:watch": "tailwindcss -i ./src/styles/tailwind.css -o ./public/dist/output.css --watch",
  "dev": "concurrently \"vite\" \"eleventy --input=src --output=public/dist --serve\" \"yarn tailwind:watch\"",
  "build": "eleventy --input=src --output=public/dist && vite build && yarn tailwind:build"
},
  "dependencies": {
    "@alpinejs/intersect": "^3.14.9",
    "@fancyapps/ui": "^6.0.16",
    "@types/alpinejs": "^3.13.11",
    "alpinejs": "^3.14.9",
    "dotenv": "^17.2.0",
    "swiper": "^11.2.10"
  }
}
